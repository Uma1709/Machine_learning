      "cell_type": "code",
      "execution_count": null,
      "id": "23e3f38f",
      "metadata": {
        "id": "23e3f38f"
      },
      "outputs": [],
      "source": [
        "def kernel(data, point, xmat, k):\n",
        "    m,n = np.shape(xmat)\n",
        "    ws = np.mat(np.eye((m)))\n",
        "    for j in range(m):\n",
        "        diff = point - data[j]\n",
        "        ws[j,j] = np.exp(diff*diff.T/(-2.0*k**2))\n",
        "    return ws"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "id": "fdbf678f",
      "metadata": {
        "id": "fdbf678f"
      },
      "outputs": [],
      "source": [
        "def local_weight(data, point, xmat, ymat, k):\n",
        "    wei = kernel(data, point, xmat, k)\n",
        "    return (data.T*(wei*data)).I*(data.T*(wei*ymat.T))"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "id": "d211605e",
      "metadata": {
        "id": "d211605e"
      },
      "outputs": [],
      "source": [
        "def local_weight_regression(xmat, ymat, k):\n",
        "    m,n = np.shape(xmat)\n",
        "    ypred = np.zeros(m)\n",
        "    for i in range(m):\n",
        "        ypred[i] = xmat[i]*local_weight(xmat, xmat[i],xmat,ymat,k)\n",
        "    return ypred\n"
      ]
    },
